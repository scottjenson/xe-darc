{
  "name": "darc-e2e-tests",
  "version": "1.0.0",
  "description": "E2E tests for Darc Browser using Playwright",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:clipboard": "playwright test 004-clipboard-history",
    "report": "playwright show-report",
    "install:playwright": "playwright install chromium"
  },
  "keywords": [
    "e2e",
    "playwright",
    "testing"
  ],
  "devDependencies": {
    "@playwright/test": "^1.48.0"
  }
}
